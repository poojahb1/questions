#include <bits/stdc++.h>

using namespace std;

int M=4, N=5;
void print_common_ele(int arr[4][5]){
    unordered_map<int, int> mp;
    for(int i=0;i<N;i++){
        mp[arr[0][i]] = 1;
    }
    for(int i=1;i<M;i++){
        for(int j=0;j<N;j++){
            if(mp[arr[i][j]]==i){
                mp[arr[i][j]] = i+1;
            }
            if(i==M-1 && mp[arr[i][j]]==M){
                cout<<arr[i][j]<<" ";
            }
        }
    }
}

int main()
{
    // cout<<"Hello World";
    int mat[M][N] =
    {
        {1, 2, 1, 4, 8},
        {3, 7, 8, 5, 1},
        {8, 7, 7, 3, 1},
        {8, 1, 2, 7, 9},
    };
 
    print_common_ele(mat);

    return 0;
}
