unordered_map<int,int>mp;
        int count=0;
        for(int i=0;i<n;i++){
            int num = arr[i];
            int toFind = K-num;
            if(mp.find(toFind)!=mp.end()){
               int temp = mp[toFind];
                count+=temp;
            }
            mp[num]++;
        }
        return count;
